  <meta charset="UTF-8">
  <%= favicon_link_tag 'favicon.png' %>

  <title>Recipe Box</title>

  <style>
    html {
      box-sizing: border-box;
    }

    *, *:before, *:after {
      box-sizing: inherit;
    }

    body {
      /*background-color: #DDA8C0;*/
      background-color: #4758c3;
      color: #fff;
      margin: 0;
    }

    .logo {
      padding: 15px;
    }

    ::-moz-selection { background: #4776bd; }
    ::selection { background: #4776bd; }

    .logo,
    .jewel {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      width: 600px;
      max-width: 100%;
    }

    .jewel {
      width: 100px;
      margin-top: -150px;
    }

    .jewel * {
      fill: transparent;
      stroke: #fff;
      stroke-width: 0.5px;
      /*stroke-linecap: round;*/
      /*stroke-linejoin: round;*/
      /*stroke-dasharray: 200;*/
      /*stroke-dashoffset: 200;*/
      /*-webkit-animation: dash 1s linear forwards;*/
    }


    .jewel:hover * {
      /*fill: #f2d1e0;*/
      fill: #FF5C5C;
    }

    .copy,
    .copy-2,
    .copy-3 {
      font-family: sans-serif;
      line-height: 1.5;
      width: 100%;
      background-color: #D77BB9;
      padding:5px;
    }

    .copy-2 {
      background-color: #4758c3;
    }

    .copy-3 {
      background-color: #000;
    }

  </style>
